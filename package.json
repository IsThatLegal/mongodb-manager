{
  "name": "mongodb-cluster-manager",
  "version": "1.0.0",
  "description": "Professional MongoDB cluster management tool with CLI, web dashboard, and automation features",
  "main": "lib/index.js",
  "bin": {
    "mm": "./bin/mm",
    "mongodb-manager": "./bin/mm"
  },
  "scripts": {
    "start": "node lib/index.js",
    "dev": "nodemon lib/index.js",
    "web": "node web/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --verbose",
    "test:ci": "jest --coverage --ci --watchAll=false --testResultsProcessor=jest-sonar-reporter",
    "lint": "eslint lib/**/*.js bin/**/*.js web/**/*.js tests/**/*.js",
    "lint:fix": "eslint lib/**/*.js bin/**/*.js web/**/*.js tests/**/*.js --fix",
    "docs": "jsdoc -d docs/api lib/**/*.js",
    "validate": "npm run lint && npm run test:coverage",
    "precommit": "npm run validate"
  },
  "keywords": [
    "mongodb",
    "cluster",
    "management",
    "database",
    "cli",
    "dashboard",
    "backup",
    "monitoring"
  ],
  "author": "MongoDB Cluster Manager",
  "license": "MIT",
  "dependencies": {
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "cli-table3": "^0.6.3",
    "boxen": "^5.1.2",
    "figlet": "^1.7.0",
    "socket.io": "^4.7.4",
    "chart.js": "^4.4.1",
    "moment": "^2.29.4",
    "yargs": "^17.7.2",
    "archiver": "^6.0.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "extract-zip": "^2.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jsdoc": "^4.0.2",
    "supertest": "^6.3.3",
    "jest-sonar-reporter": "^2.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/mongodb-cluster-manager.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/mongodb-cluster-manager/issues"
  },
  "homepage": "https://github.com/your-username/mongodb-cluster-manager#readme"
}